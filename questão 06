#include <iostream>
using namespace std;

int parametro(int a, int b) {
    return a + b;
}

int main() {
    int numero1, numero2;
   
    cout << "Digite o primeiro numero: " ;
    cin >> numero1;

    cout << "Digite o segundo numero: ";
    cin >> numero2;

    int soma = parametro(numero1, numero2);

    cout << "A soma dos numeros Ã© " << soma << endl;

    return 0;
}
